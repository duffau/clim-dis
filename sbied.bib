@Article{Roberts1997,
  Title                    = {Weak Convergence and Optimal Scaling of Random Walk {M}etropolis Algorithms},
  Author                   = {Roberts, G. O. and Gelman, A. and Gilks, W. R.},
  Journal                  = {The Annals of Applied Probability},
