@Article{Renshaw1998,
  Title                    = {Can Markov chain Monte Carlo be usefully applied to stochastic processes with hidden birth times?},
  Author                   = {Eric Renshaw and Gavin J Gibson},
  Journal                  = {Inverse Prob.},
  Year                     = {1998},
  Number                   = {6},
  Pages                    = {1581--1606},
  Volume                   = {14},

  Abstract                 = {This paper examines the power of Markov chain Monte Carlo methods to tackle the `inverse' problem of stochastic population modelling. Namely, given a partial series of event-time observations, believed governed by a known process, what range of model parameters might plausibly explain it? This problem is first introduced in the simple context of an immigration-death process, in which only deaths are recorded, and is then extended through the introduction of birth, standard and power-law logistic growth, and an `odd-even effects' quantum optics model. The results show that simple Metropolis Hastings samplers can be applied to provide useful information on models containing a high degree of complexity. Specific problems highlighted include: the potentially poor mixing qualities of simple Metropolis Hastings samplers; and, that heavily non-symmetric full likelihood surfaces may inflict substantial bias on their associated marginal distributions.},
